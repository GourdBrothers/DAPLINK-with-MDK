Nstatic uint8_t target_unlock_sequence(void)
N{
N    uint32_t val;
N
N    // read the device ID
N    if (!swd_read_ap(MDM_IDR, &val)) {
X    if (!swd_read_ap(0x010000fc, &val)) {
N        return 0;
N    }
N
N    // verify the result
N    if (val != MDM_ID) {
X    if (val != 0x001c0020) {
N        return 0;
N    }
N
N    if (!swd_read_ap(MDM_STATUS, &val)) {
X    if (!swd_read_ap(0x01000000, &val)) {
N        return 0;
N    }
N
N    // flash in secured mode
N    if (val & (1 << 2)) {
N        // hold the device in reset
N        swd_set_target_reset(1);
N
N        // write the mass-erase enable bit
N        if (!swd_write_ap(MDM_CTRL, 1)) {
X        if (!swd_write_ap(0x01000004, 1)) {
N            return 0;
N        }
N
N        while (1) {
N            // wait until mass erase is started
N            if (!swd_read_ap(MDM_STATUS, &val)) {
X            if (!swd_read_ap(0x01000000, &val)) {
N                return 0;
N            }
N
N            if (val & 1) {
N                break;
N            }
N        }
N
N        // mass erase in progress
N        while (1) {
N            // keep reading until procedure is complete
N            if (!swd_read_ap(MDM_CTRL, &val)) {
X            if (!swd_read_ap(0x01000004, &val)) {
N                return 0;
N            }
N
N            if (val == 0) {
N                break;
N            }
N        }
N    }
N
N    return 1;
N}
